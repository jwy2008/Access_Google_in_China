{
	"manifest_version": 3,
	"name": "Google访问",
	"version": "1.0.0",
	"description": "在中国访问google.com!",
	"icons": {
		"4":"image/1.png",
		"16":"image/1.png",
		"128":"image/1.png"
	},
	"content_scripts": [
		{
			"matches": [
				"*://www.google.cn/*"
			],
			"js": [
				"jquery-3.6.0.min.js",
				"popup.js"
			]
		}
	],
	"action": {
		"default_title": "google"
	}
}